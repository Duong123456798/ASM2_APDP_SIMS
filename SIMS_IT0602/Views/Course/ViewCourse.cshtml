@model IEnumerable<Course>
<h1>Hello , Welcome to Course Homepage,Teacher!</h1>
<table class="table">
    <thead>
        <tr>
            <td>ID</td>
            <td>Name</td>
            <td>Class</td>
            <td>Major</td>
            <td>Lecturer</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Class</td>
                <td>@item.Major</td>
                <td>@item.Lecturer</td>        
            </tr>
        }
    </tbody>
</table>

<ul>
    
    <li> <a class="btn btn-sm btn-primary" asp-controller="Teacher" asp-action="Index">Back to HomePage</a></li>
</ul>
