@model IEnumerable<Teacher>
<h1>Welcome to Teacher List,Student!!</h1>
<table class="table">
    <thead>
        <tr>
            <td>ID</td>
            <td>Name</td>
            <td>Date of Birth</td>
            <td>Major</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.DoB</td>
                <td>@item.Major</td>
            </tr>
        }
    </tbody>
</table>
<ul>
    <li>
        <a class="btn btn-sm btn-primary" asp-controller="Student" asp-action="Index">Back to HomePage</a>
    </li>
</ul>
